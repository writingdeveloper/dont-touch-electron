{
  "name": "dont-touch-electron",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "description": "Face-touch detection app for habit improvement using MediaPipe",
  "author": "writingdeveloper",
  "license": "MIT",
  "private": true,
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "scripts": {
    "dev": "cross-env ELECTRON_RUN_AS_NODE= vite",
